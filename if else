#include <iostream>
using namespace std;
int main() {
    
    string nam;
    int marks;
    
    cout<< "Enter your name: ";
    cin>>nam;
    cout<<"please enter your percentage: ";
    cin>>marks;
    
    if(marks >= 80){
        cout<<"your grade is A+";
        
    }
    else if (marks >= 70 && marks <= 80){
        cout<<"your grade is A";
    }
    else if (marks >= 60 && marks <= 70){
        cout<<"your grade is A";
    }
    else if (marks >= 50 && marks <= 60){
        cout<<"your grade is A";
    }
    
    
    else (marks > 50);{
        cout<<"fail ho ustaad" ;
    }  

   
   
   
   
}
