
#include <iostream>
using namespace std;

int sum(int a, int b);


int main(){
    int x, y;
    cout<<"Enter two numbers: ";
    cin>>x>>y;
    
    
    int result = sum(x,y);
    cout<<"the sum of "<< x <<" and "<< y <<" is: "<<result<<endl;
    
    return 0;
}


int sum(int a, int b){
    return a + b;
}
