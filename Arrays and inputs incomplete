#include<iostream>
using namespace std;
int main()
{
     int i, j, matrix[20][20], row, col;
    
    
    cout<<"Enter number of rows of matrix: ";
cin>>row;
cout<<"Enter number of column of matrix: ";
cin>>col;
    
    
    cout<<"Enter the elements of array:"<<endl;
for(i=0;i<row;i++){
for(j=0;j<col;j++){
cout<<"Enter the elements of matrix["<<i<<"]["<<j<<"]:";
cin>>matrix[i][j];
    
}

}
}
